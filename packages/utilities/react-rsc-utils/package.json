{
  "name": "@cloudstack-design/react-rsc-utils",
  "version": "1.0.0",
  "description": "A set of utilities for react on server side for CloudStack Design",
  "keywords": [
    "react-rsc-utils",
    "cloudstack-design"
  ],
  "author": "CloudStackDesign <support@cloudstack-design.com>",
