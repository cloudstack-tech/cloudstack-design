{
  "name": "@cloudstack-design/shared-utils",
  "version": "1.0.0",
  "description": "A set of Cloustack Design utilities",
  "keywords": [
    "shared-utils"
  ],
  "author": "CloudStackDesign <support@cloudstack-design.com>",
  "homepage": "https://cloudstack-design.com",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudstack-tech/cloudstack-design.git",
    "directory": "packages/utilities/shared-utils"
  },
  "bugs": {
    "url": "https://github.com/cloudstack-tech/cloudstack-design/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "devDependencies": {
    "clean-package": "2.2.0"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}